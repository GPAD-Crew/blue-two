{
  "name": "blue-two",
  "version": "2.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -b",
    "prestart": "npm run build",
    "start": "node ./dist",
    "lint": "eslint --ext=ts ./src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky install"
  },
  "license": "CC-BY-4.0",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm start"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@tsconfig/node16": "^1.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/luxon": "^1.27.1",
    "@types/node": "latest",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "better-sqlite3": "^7.5.1",
    "cookie": "^0.5.0",
    "cron": "^3.0.0",
    "csv-stringify": "^5.6.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "luxon": "^1.27.0",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.10",
    "pg": "^8.7.3",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typeorm": "^0.3.6",
    "typescript": "4"
  },
  "_moduleAliases": {
    "~": "dist"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "husky": "^7.0.0"
  }
}
